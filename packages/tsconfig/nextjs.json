{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "composite": false,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strict": true,
    "strictPropertyInitialization": false
  }
}